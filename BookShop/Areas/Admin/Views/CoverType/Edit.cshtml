@model CoverType

@{
    ViewData["Title"] = "Edit";
}

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Edit CoverType</h2>
            <hr />
        </div>
        @* <div asp-validation-summary="All"></div>*@
        <div class="mb-3">
            <label asp-for="Name">Name</label>
            <input asp-for=Name class="form-control" />
            
        </div>

        
        <button type="submit" class="btn btn-dark disabled" style="width:150px">Edit</button>
        <a asp-controller="CoverType" asp-action="Index" class="btn btn-dark disabled" style="width:150px;">
            Back To List

        </a>

    </div>
</form>

@section Scripts {
    @{
        <partial name="_ValidationScriptsPartial" />
    }

}
